{
  "manifest_version": 2,

  "name": "Property Reporter",
  "description": "This extension assists in collecting and summarizing property information.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "resources/icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "<all_urls>",
    "tabs",
    "activeTab",
    "storage"
  ]
}
